{
  "editor.formatOnSave": true,
  "html.format.indentInnerHtml": true,
  "html.format.indentHandlebars": true,
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.minimap.enabled": false,
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "workbench.colorCustomizations": {
    "statusBar.background": "#2f3546",
    "statusBar.noFolderBackground": "#2f3546",
    "statusBar.debuggingBackground": "#2f3546"
  },
  "vim.operatorPendingModeKeyBindingsNonRecursive": [],
  "vim.vimrc.enable": true,
  "vim.vimrc.path": "C:\\Users\\Laptop\\AppData\\Roaming\\Code\\User\\.vimrc",
  "vim.leader": ",",
  "vim.insertModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "j", "k"],
      "after": ["<Esc>"]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "H"],
      "after": ["^"]
    },
    {
      "before": ["<leader>", "L"],
      "after": ["$"]
    }
  ],
  "window.zoomLevel": 1,
  "terminal.integrated.defaultProfile.windows": "Ubuntu-20.04 (WSL)",
  "editor.lineNumbers": "relative",
  "workbench.editorAssociations": {
    "*.pdf": "default"
  },
  "window.menuBarVisibility": "compact",
  "workbench.colorTheme": "Palenight Theme",
  "[typescriptreact]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "editor.wordWrap": "on",
  "workbench.sideBar.location": "right"
}
